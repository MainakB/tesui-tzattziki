    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "proseWrap": "always",
    "arrowParens": "avoid",
    "quoteProps": "consistent",
    "overrides": [{"files": ".*rc", "options": {"parser": "json"}}]
}
