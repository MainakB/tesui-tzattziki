{
  "name": "webdriverio-tests",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@kaniamb/tzatziki-bl": "^0.1.2",
    "@kaniamb/tzatziki-core": "^1.1.1",
    "grunt-continue": "^0.1.0",
    "grunt-shell-spawn": "0.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "4.7.4"
  },
  "scripts": {
    "compile": "npm run clean:pretest && npm run compile:all",
    "clean:pretest": "npm run clean:build & npm run clean:reports & npm run clean:videos",
    "clean:build": "rimraf dist",
    "clean:reports": "rimraf Reports",
    "clean:videos": "rimraf _results_",
    "compile:all": "tsc --diagnostics",
    "lint:watch": "onchange \"Test_Modules/**/*.ts\" -- onerror \"eslint Test_Modules/**/*.ts --fix\""
  }
}
